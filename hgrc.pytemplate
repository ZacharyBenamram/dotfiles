[ui]
username = $codehosting_email

[auth]
codehosting.prefix = https://code.google.com/
codehosting.username = $codehosting_email
codehosting.password = $codehosting_password

[alias]
blame = annotate --user --changeset

[extensions]
mq =
color =
bookmarks =

[color]
status.added = green bold
status.removed = red bold
status.deleted = cyan bold
status.unknown = magenta bold
status.ignored = black bold

[bookmarks]
track.current = True
